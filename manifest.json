{
    "name":"Apply Ease",
    "version":"1.0",
    "description":"An Extension to apply jobs with ease",
    "manifest_version":3,
    "action":{
        "default_popup":"index.html",
        "default_title":"Apple Ease"
    },
    "permissions":[
        "scripting", "activeTab","storage"
    ],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "run_at":"document_start",
          "js": ["contentscript.js"]
        }
    ]
}